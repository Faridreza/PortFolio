@model IEnumerable<Experience>
<div class="container">
    <div class="col-md-12">
        <h4>02</h4>
        <h1 class="size-50">My <br /> Experience</h1>
        <div class="h-50"></div>
    </div>
    <div class="col-md-12">
        <ul class="timeline">
            @foreach (Experience item in Model)
            {
                <li class="timeline-event" data-aos="fade-up" data-aos-delay=".1">
                    <label class="timeline-event-icon"></label>
                    <div class="timeline-event-copy">
                        <h3>@item.Title</h3>
                        <h4>@item.Level</h4>
                        <p>@item.Content</p>
                    </div>
                </li>
            }
        </ul>
    </div>
</div>